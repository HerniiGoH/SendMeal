<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="isi.dam.sendmeal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity_crear_pedido"></activity>
        <activity android:name=".BusquedaItem" />
        <activity android:name=".ListaResultados" />
        <activity android:name=".EditarItem" />
        <activity android:name=".ListaItems" />
        <activity android:name=".ListaItemsPedido" />
        <activity android:name=".ListaPedido" />
        <activity
            android:name=".CrearItem"
            android:parentActivityName=".HOME" />
        <activity android:name=".HOME">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PantallaUsuario"
            android:parentActivityName=".HOME" />
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".HOME" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>